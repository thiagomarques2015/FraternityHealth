<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.fraternityhealth">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.SpecialtyActivity"
            android:label="@string/title_activity_speacilty"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.LocationActivity"
            android:label="@string/title_activity_location"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.MedicalCenterActivity"
            android:label="@string/title_activity_medical_center"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".view.DoctorActivity"></activity>
    </application>

</manifest>